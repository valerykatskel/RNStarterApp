{
  "name": "RNStarterApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 16 Pro\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "start-android-emulator": "export ANDROID_SDK_ROOT=$HOME/Library/Android/sdk && $ANDROID_SDK_ROOT/emulator/emulator -avd Pixel_9_Pro_API_35 &",
    "start-ios-simulator": "open -a Simulator && xcrun simctl boot \"iPhone 16 Pro\"",
    "android-dev": "npm run start-android-emulator && sleep 10 && npm run android",
    "ios-dev": "npm run start-ios-simulator && sleep 5 && npm run ios"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
